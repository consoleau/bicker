{
  "name": "bicker",
  "version": "1.3.3",
  "private": true,
  "engines": {
    "node": ">=6.9.5"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "**/__tests__/*.test.js"
    ]
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-angularjs-annotate": "^0.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "7.2.0",
    "bower": "^1.4.1",
    "browserify": "13.0.0",
    "eslint": "^3.10.2",
    "eslint-plugin-console-saas-rules": "consoleau/eslint-plugin-console-saas-rules#1.0.11",
    "eslint-plugin-no-only-tests": "^1.1.0",
    "express": "4.13.4",
    "grunt": "^0.4.5",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-uglify": "^2.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-express-server": "^0.5.1",
    "grunt-karma": "^0.12.1",
    "grunt-ng-annotate": "^3.0.0",
    "jasmine-core": "^2.3.4",
    "jest": "^19.0.0",
    "karma": "^0.13.19",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-ng-scenario": "^0.1.0",
    "karma-spec-reporter": "^0.0.31",
    "lodash": "4.0.1",
    "traverse": "^0.6.6"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            [
              "angularjs-annotate",
              {
                "explicitOnly": true
              }
            ]
          ]
        }
      ]
    ]
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start test/karma/karma.conf.js",
    "get_deps": "bower install && npm install"
  }
}
